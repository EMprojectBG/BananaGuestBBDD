package com.netmind.db;

import java.util.List;

import com.netmind.models.Maquillaje;

public abstract class ProyectoDAO extends DAO {
	public abstract ProyetoB getProyecto(int idProyecto);
	public abstract List<ProyetoB> getUserProyecto(int uid);
	public abstract List<ProyetoB> getProyecto();
	public abstract boolean delProyecto(int mid);
	public abstract boolean insertProyecto(ProyetoB miProyecto);
	public abstract boolean updateProyecto(ProyetoB miProyecto);
}
